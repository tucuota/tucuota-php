{
  "name": "tucuota/tucuota-php",
  "description": "TuCuota PHP Client",
  "keywords": [
    "tucuota",
    "payment processing",
    "api"
  ],
  "homepage": "https://tucuota.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "TuCuota contributors",
      "homepage": "https://github.com/tucuota/tucuota-php/contributors"
    }
  ],
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "~6.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "friendsofphp/php-cs-fixer": "2.17.1"
  },
  "autoload": {
    "psr-4": {
      "TuCuota\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TuCuota\\": [
        "tests/"
      ]
    }
  }
}
